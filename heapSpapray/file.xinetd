service heap
{
    protocol       = tcp
    disable        = no
    port           = 8890
    flags          = REUSE
    socket_type    = stream
    wait           = no
    user           = heapspapray
    server         = /usr/bin/timeout
    server_args    = 10 /home/heapSpapray/heapSpapray
    type           = UNLISTED
}
